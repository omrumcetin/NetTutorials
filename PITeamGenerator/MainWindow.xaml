<Window x:Class="teamgenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:teamgenerator"
        mc:Ignorable="d"
        Loaded="OnLoad"
        Title="P.I. Team Generator" Height="700" Width="1300" WindowStyle="ThreeDBorderWindow" FontFamily="Microsoft JhengHei">
    <Grid>
        <Button Content="Commit Match" HorizontalAlignment="Left" Margin="1112,609,0,0" VerticalAlignment="Top" Width="170" Height="50" RenderTransformOrigin="0.5,0.5"/>
        <Button Content="Generate Teams" HorizontalAlignment="Left" Margin="933,609,0,0" VerticalAlignment="Top" Width="170" Height="50" RenderTransformOrigin="0.5,0.5" Click="Button_Click"/>
        <ListBox x:Name="ListBoxTeamOverview" HorizontalAlignment="Left" Height="659" Margin="10,0,0,0" VerticalAlignment="Top" Width="300" BorderThickness="0" Background="White" SelectionMode="Multiple"/>

    </Grid>
</Window>
